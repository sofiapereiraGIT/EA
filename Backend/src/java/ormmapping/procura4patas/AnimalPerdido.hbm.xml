<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="procura4patas.AnimalPerdido" table="AnimalPerdido" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="animalPerdido" column="AnimalID" class="procura4patas.Animal" cascade="save-update,lock,delete" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="concelho" column="Concelho" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
